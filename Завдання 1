import matplotlib.pyplot as plt
import numpy as np

x = np.linspace(-2, 5, 400)
y = x * np.sin(5 * x)

plt.plot(x, y, label='x*sin(5x)', color="red", linewidth=5)

plt.title('Графік функції Y(x)=x*sin(5x)', fontsize=15)# назва графіка
plt.xlabel('x', fontsize=12, color='blue')# позначення вісі абсцис
plt.ylabel('Y(x)', fontsize=12, color='blue')# позначення вісі ординат

plt.legend()
plt.grid(True)

plt.show()
